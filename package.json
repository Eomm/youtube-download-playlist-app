{
  "name": "yt-dl-playlist-app",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate-cert": "greenlock certonly --standalone --acme-version draft-11 --acme-url https://acme-staging-v02.api.letsencrypt.org/directory  --agree-tos --email behemoth89@gmail.com --domains yt-dl-playlist-app.eomm.app --community-member --config-dir ./cert",
    "build-module": "electron-rebuild -v 3.0.10 -w",
    "build": "electron-builder build -wml",
    "build-win": "electron-builder build -w",
    "standard:fix": "standard --fix"
  },
  "build": {
    "appId": "app.eomm.yt-dl-playlist-app",
    "directories": {
      "buildResources": "."
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg"
    },
    "win": {},
    "linux": {
      "category": "Utility"
    }
  },
  "dependencies": {
    "bulma": "^0.7.2",
    "electron-window-state": "^5.0.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "yt-dl-playlist": "^1.0.2"
  },
  "devDependencies": {
    "electron": "^3.0.13",
    "electron-builder": "^20.38.4",
    "electron-rebuild": "^1.8.2",
    "electron-reload": "^1.4.0",
    "greenlock-cli": "^2.3.1",
    "standard": "^12.0.1"
  },
  "keywords": [
    "electron",
    "youtube",
    "download",
    "playlist",
    "mp3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Eomm/youtube-download-playlist-app.git"
  },
  "author": "Manuel Spigolon <behemoth89@gmail.com> (https://github.com/Eomm)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Eomm/youtube-download-playlist-app/issues"
  },
  "homepage": "https://github.com/Eomm/youtube-download-playlist-app#readme"
}